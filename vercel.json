{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "trailingSlash": false,
    "cleanUrls": true,
    "redirects": [
        {
          "source": "/ru/dictionary/:letter",
          "destination": "https://urssivar.com/language/ru/dictionary/:letter",
          "permanent": true
        },
        {
          "source": "/dictionary",
          "destination": "https://urssivar.com/language/dictionary/intro",
          "permanent": true
        },
        {
          "source": "/ru/dictionary",
          "destination": "https://urssivar.com/language/ru/dictionary/intro",
          "permanent": true
        },
        {
          "source": "/dictionary/:letter",
          "destination": "https://urssivar.com/language/dictionary/:letter",
          "permanent": true
        },
        {
          "source": "/ru/(.*)",
          "destination": "https://urssivar.com/language/ru/",
          "permanent": true
        },
        {
          "source": "/(.*)",
          "destination": "https://urssivar.com/language/",
          "permanent": true
        }
    ]
}
